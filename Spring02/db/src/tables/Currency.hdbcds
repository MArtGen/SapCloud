namespace javaCFMTA;

using javaCFMTA::Course;

entity Currency {

    @Comment: 'ID of Currency'
    key cuid : Integer64;
    @Comment: 'Name'
    name : String(10);
    @Comment: 'Created By'
    createdby : String(100);
    @Comment: 'Created On'
    createdon : String(30);

    toCourse: Association[*] to Course on toCourse.cuid = cuid;

};