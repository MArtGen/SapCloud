namespace javaCFMTA;

using javaCFMTA::Currency;

entity Course {

    @Comment: 'ID of Course'
    key coid : Integer64;
    @Comment: 'Currency ID'
    cuid : Integer64;
    @Comment: 'Course Date'
    date : String(30);
    @Comment: 'Course Value'
    value : String(10);
    @Comment: 'Created By'
    createdby : String(100);
    @Comment: 'Created On'
    createdon : String(30);

    toCurrency: Association[1] to Currency on toCurrency.cuid = cuid;

};