{
  "welcomeFile": "/cp.portal",
  "authenticationMethod": "none",
  "routes": [
    {
      "source": "/api/(.*)",
      "target": "$1",
      "destination": "lev_srv",
      "authenticationType": "xsuaa"
    },
    {
      "source": "^/(.*)$",
      "target": "$1",
      "service": "html5-apps-repo-rt",
      "authenticationType": "xsuaa"
    }
  ]
}
