{
  "welcomeFile": "/cur_display/index.html",
  "authenticationMethod": "none",
  "routes": [
    {
      "source": "^/sapui5_resources/(.*)$",
      "target": "$1",
      "destination": "sapui5_provider"
    },
    {
      "source": "/api/(.*)",
      "target": "$1",
      "destination": "lev_srv",
      "authenticationType": "xsuaa"
    },
    {
      "source": "/crs/(.*)",
      "target": "/$1",
      "destination": "crs"
    },
    {
      "source": "^/s4c/(.*)",
      "target": "/$1",
      "destination": "S4C"
    },
    {
      "source": "^/(.*)$",
      "target": "$1",
      "service": "html5-apps-repo-rt",
      "authenticationType": "xsuaa"
    }
  ]
}
